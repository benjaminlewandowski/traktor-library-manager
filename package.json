{
  "name": "traktor-library-manager",
  "version": "0.9.7",
  "private": true,
  "description": "Manage your Traktor Pro 3 library with ease! Build with love, with the help of Vue, Tailwind and Electron, and many more packages",
  "author": {
    "name": "Sefrijn",
    "email": "info@howaboutyes.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm-run-all lint:*",
    "lint:vue": "vue-cli-service lint",
    "lint:stylelint": "stylelint ./src/css/*.scss",
    "lint:eslint": "eslint ./src/**/*.{js,vue}",
    "fix": "eslint --fix ./src/**/*.*",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write ./src/**/*.{js,vue,scss}",
    "format:stylelint": "stylelint ./src/css/*.scss --fix",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve --trace-warnings",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@he-tree/vue3": "^1.2.9",
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^6.7.96",
    "@syncfusion/ej2-vue-navigations": "^20.1.57",
    "ag-grid-community": "^27.3.0",
    "ag-grid-vue3": "^27.3.0",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "floating-vue": "2.0.0-beta.16",
    "lodash.clonedeep": "^4.5.0",
    "music-metadata": "^7.12.3",
    "postcss": "^8.4.14",
    "qs": "^6.10.3",
    "sharp": "^0.30.5",
    "slugify": "^1.6.5",
    "systeminformation": "^5.11.15",
    "tailwindcss": "^3.0.24",
    "throttle-debounce": "^5.0.0",
    "tinykeys": "^1.4.0",
    "vue": "^3.2.36",
    "vue-class-component": "next",
    "vue-slider-component": "next",
    "vue-spinner": "^1.0.4",
    "vuex": "^4.0.2",
    "wavesurfer.js": "^6.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.36",
    "electron": "^19.0.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.1",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-prettier": "^2.0.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-tailwind": "~3.0.0"
  }
}
